      <PasswordField fx:id="txtPassword" layoutX="50.0" layoutY="138.0" prefHeight="25.0" prefWidth="209.0" promptText="Password" />
      <Label fx:id="lblStatus" layoutX="50.0" layoutY="31.0" text="Status" textFill="#f81414">
